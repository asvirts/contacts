<% if user_signed_in? %>

<h1>Add new contact</h1>
<%= link_to "Back to contacts", contacts_path %>

<div class="mt-4">
<%= render "form", contact: @contact %>
</div>

<% else %>

<div class="alert alert-warning my-2" role="alert">
  You need to sign in or sign up before continuing.
</div>

<% end %>