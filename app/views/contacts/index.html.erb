<p style="color: green"><%= notice %></p>

<h1>Contacts</h1>
<%= link_to "New contact", new_contact_path %>

<table class="table">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Threads</th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %> <%= render contact %> <% end %>
  </tbody>
</table>
